<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Edit Form</title>
    <% include bs %>
    <style>
      .container1 {
        width: 400px;
        margin: 0 auto;
        margin-top: 10px;
      }
      h1 {
        font-size: 30px;
        text-align: center;
      }
    </style>
</head>
<body>
    <div class="container1">
      <h1>Edit Page</h1>
      <form method="post" action="/edit">
        <% if(locals.oil) {%>
          <% Object.values(oil).forEach(function (oil) {%>
          <div class="form-group">
            <input type="hidden" class="form-control" name="date" id="exampleInputEmail1" value="<%= oil['Date'] %>">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Disel</label>
            <input type="text" class="form-control" name="disel" id="exampleInputPassword1" value="<%= oil['Disel'] %>">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Bensin Gasohol E20</label>
            <input type="text" class="form-control" name="e20" id="exampleInputPassword1" value="<%= oil['Bensin Gasohol E20'] %>">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Gasohol 91</label>
            <input type="text" class="form-control" name="g91" id="exampleInputPassword1" value="<%= oil['Gasohol 91'] %>">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Gasohol 95</label>
            <input type="text" class="form-control" name="g95" id="exampleInputPassword1" value="<%= oil['Gasohol 95'] %>">
          </div>
          <button type="submit" class="btn btn-primary">Edit</button>
        <% })} %> 
        </form>
    </div>
</body>
</html>